{
  "name": "better_than_predator",
  "title": "Better than Predator",
  "version": "1.0.0",
  "description": "Is it better than Predator?",
  "homepage": "http://betterthanpredator.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/linssen/better_than_predator.git"
  },
  "contributors": [
    {
      "name": "Wil Linssen",
      "website": "http://linssen.me/"
    }
  ],
  "author": "Wil Linssen",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "concurrently": "^3.5.0",
    "node-sass": "^4.5.3",
    "vue": "^2.4.2",
    "vueify": "^9.4.1",
    "vuex": "^2.3.1",
    "watchify": "^3.9.0"
  },
  "scripts": {
    "build": "browserify ./static/scripts/app.js -o ./static/dist/scripts/bundle.js -t babelify -t vueify",
    "watch:scripts": "watchify ./static/scripts/app.js -o ./static/dist/scripts/bundle.js -t babelify -t vueify -v",
    "css": "node-sass ./static/styles/main.scss -o ./static/dist/styles",
    "watch:css": "node-sass ./static/styles/main.scss -o ./static/dist/styles -w",
    "watch": "concurrently --kill-others 'yarn run watch:css' 'yarn run watch:scripts'",
    "test": ""
  },
  "dependencies": {}
}
